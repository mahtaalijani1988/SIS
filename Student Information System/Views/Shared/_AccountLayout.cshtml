<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>
    @Styles.Render("~/Main/css")
    <!--[if IE 7]>
    <link href="~/Content/css/ie/ie7.css" rel="stylesheet" />
        <![endif]-->
    <!--[if IE 8]>
    <link href="~/Content/css/ie/ie8.css" rel="stylesheet" />
        <![endif]-->
    <!--[if IE 9]>
    <link href="~/Content/css/ie/ie9.css" rel="stylesheet" />
        <![endif]-->
    <!--fav and touch icons -->
    <link rel="shortcut icon" href="~/Content/ico/favicon.ico" />

    <link href="~/Content/sweet-alert.css" rel="stylesheet" />

</head>
<body>
    <div id="main-content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </div>
    <script src="~/Content/js/jquery.js"></script>
    @Scripts.Render("~/js")


    <script src="~/Content/sweet-alert.js"></script>

    <script src="~/Content/js/respond.min.js"></script>
    <script src="~/Content/js/ios-orientationchange-fix.js"></script>

    @RenderSection("JavaScript", required: false)
</body>
</html>

